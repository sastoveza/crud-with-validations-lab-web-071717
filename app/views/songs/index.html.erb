<h1>All Songs</h1>
<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Released?</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= link_to "Show", song %> | <%= link_to "Edit", edit_song_path(song) %> | <%= link_to "Delete", song, method: :delete %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<p>
  <%= link_to "New Song", new_song_path %>
</p>